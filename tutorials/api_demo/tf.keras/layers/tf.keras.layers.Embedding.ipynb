    "# emb_layer，输入可以是float，也可以是int哦\n",
    "import tensorflow as tf\n",
    "import numpy as np\n",
    "emb_layer=tf.keras.layers.Embedding(input_dim=4,output_dim=4)\n",
    "input_arr=np.random.randint(0,3,size=[10,3]).astype(np.float32)\n",
    "\n",
    "emb_layer(input_arr)"
